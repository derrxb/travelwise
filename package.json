{
  "name": "zelo.run",
  "private": true,
  "description": "- [Zelo.Run Docs](https://zelo.run)",
  "license": "",
  "type": "module",
  "sideEffects": false,
  "prisma": {
    "seed": "tsx prisma/seed.mts"
  },
  "scripts": {
    "build": "npx npm-run-all build:*",
    "build:remix": "cross-env NODE_ENV=production remix vite:build",
    "build:sitemap": "cross-env NODE_ENV=production remix-sitemap",
    "dev": "dotenv -e .env.dev -- npm-run-all build --parallel \"dev:*\"",
    "dev:build": "cross-env NODE_ENV=development remix vite:dev",
    "dev:db": "npx prisma studio",
    "dev:docker-db": "docker-compose up",
    "db:reset:dev": "dotenv -e .env.dev npx prisma migrate reset",
    "db:seed:dev": "yarn db:reset:dev && npx dotenv -e .env.dev prisma db seed",
    "db:migrate:create": "dotenv -e .env.dev npx prisma migrate dev --name",
    "db:migrate": "run-p db:migrate:*",
    "db:migrate:dev": "dotenv -e .env.dev npx prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- npx prisma db push --accept-data-loss",
    "db:gen:migrate": "dotenv -e .env.dev npx prisma migrate dev --name",
    "start": "cross-env NODE_ENV=production remix-serve ./build/server/index.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:local": "run-s db:migrate:test test:coverage:disabled",
    "test:ci": "run-s db:migrate:test npm run test:coverage:enabled",
    "test:coverage:enabled": "dotenv -e .env.test -- vitest --coverage",
    "test:coverage:disabled": "dotenv -e .env.test -- vitest "
  },
